{
  "name": "@sapient-ui/core",
  "version": "1.0.1",
  "description": "Sapient-UI Web Components library",
  "repository": {
    "type": "git",
    "url": "https://github.com/xebia-france/sapient-ui.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/core/core.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "yarn clean && stencil build --docs",
    "build:watch": "yarn clean && stencil build --docs --watch",
    "clean": "rm -fr dist loader",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "test.spec.watch": "stencil test --spec --watchAll",
    "test.e2e.watch": "stencil test --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006 --no-dll",
    "storybook:build": "build-storybook --no-dll"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@stencil/react-output-target": "^0.0.9",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-knobs": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/web-components": "^6.0.28",
    "@types/puppeteer": "5.4.0",
    "babel-loader": "8.1.0",
    "colornames": "^1.1.1",
    "lit-html": "^1.3.0",
    "node-sass": "5.0.0",
    "puppeteer": "5.4.1",
    "react-is": "^17.0.1",
    "stencil-node-sass": "1.1.3",
    "storybook-events-logger": "^2.0.5"
  },
  "gitHead": "3a935c7a15329bd0ce9942f8372ae5a22fa42fa6"
}
