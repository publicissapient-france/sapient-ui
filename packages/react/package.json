{
  "name": "sapient-ui-react",
  "sideEffects": false,
  "version": "1.0.0",
  "private": true,
  "description": "React specific wrapper for component-library",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionic.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p .",
    "rollup": "rollup -c"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "sapient-ui-core": "^1.0.0",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "sapient-ui-core": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
