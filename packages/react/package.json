{
  "name": "sapient-ui-react",
  "sideEffects": false,
  "version": "1.0.0",
  "private": true,
  "description": "React specific wrapper for component-library",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionic.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "build:watch": "npm run clean && npm run compile:watch",
    "clean": "rm -rf dist",
    "compile": "tsc -p .",
    "compile:watch": "tsc -w -p .",
    "rollup": "rollup -c"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "sapient-ui-core": "*",
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "*",
    "react-dom": "*",
    "typescript": "*"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
