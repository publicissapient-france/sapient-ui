{
  "name": "@sapient-ui/react",
  "version": "1.0.2",
  "description": "React wrappers for Sapient-UI Web Components library",
  "repository": {
    "type": "git",
    "url": "https://github.com/xebia-france/sapient-ui.git",
    "directory": "packages/react"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "build:watch": "yarn run clean && yarn run compile:watch",
    "clean": "rm -rf dist",
    "compile": "tsc -p .",
    "compile:watch": "tsc -w -p .",
    "rollup": "rollup -c"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "@sapient-ui/core": "*",
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "*",
    "react-dom": "*",
    "typescript": "*"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  },
  "gitHead": "caef4c908ae5d843751f510e67b5c2e806c81c65"
}
